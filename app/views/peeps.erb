<body style="background: url(https://www.nighthelper.com/wp-content/uploads/2016/03/nature_waterfall_summer_lake_trees_90400_3840x2160.jpg); background-size: 100%;">
<h1><font color="lightblue"> Peeps </h1>

<h2>
  Welcome, </font><font color="#091634"><%= @maker_name.capitalize! %> with user_name: <%= @maker_user_name %></font>
</h2>

<ul id='peeps'>
    <% @peeps.sort_by {|peep| peep.time }.reverse.each do |peep| %>
    <li>
      Peep:   <%= peep.peep_name  %>
      Time:   <%= peep.time  %>
      Maker name:   <%= peep.last_maker  %>
    </li>
  <% end %>
</ul>
</body>
